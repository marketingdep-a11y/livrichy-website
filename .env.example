APP_NAME="Statamic"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=https://your-domain.com
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=your_database_name
DB_USERNAME=your_database_user
DB_PASSWORD=your_database_password

# Session Configuration
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Cache Configuration
CACHE_STORE=database
CACHE_PREFIX=

# Queue Configuration
QUEUE_CONNECTION=database
QUEUE_FAILED_DRIVER=database-uuids

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Filesystem
FILESYSTEM_DISK=local

# Statamic Configuration
STATAMIC_LICENSE_KEY=
STATAMIC_PRO_ENABLED=false
STATAMIC_GIT_ENABLED=false
STATAMIC_ANTLERS_DEBUGBAR=false
STATAMIC_STACHE_WATCHER=false
STATAMIC_STATIC_CACHING_STRATEGY=full
STATAMIC_API_ENABLED=false
STATAMIC_GRAPHQL_ENABLED=false

# Statamic Eloquent Driver (if used)
STATAMIC_ELOQUENT_CONNECTION=
STATAMIC_ELOQUENT_PREFIX=

# Service URLs
IMPORT_JSON_URL=
IMPORT_JSON_TIMEOUT=10
IMPORT_JSON_RETRIES=3
IMPORT_JSON_BACKOFF=2
IMPORT_JSON_STORE=imports
IMPORT_JSON_COLLECTION=
IMPORT_JSON_COLLECTION_HANDLE=properties
IMPORT_JSON_ID_KEY=id
IMPORT_JSON_REQUIRED_FIELDS=
IMPORT_JSON_STATUS_KEY=
IMPORT_JSON_ACTIVE_STATUSES=
IMPORT_JSON_WEBSITE_ENABLE_KEY=
IMPORT_JSON_WEBSITE_ENABLE_ON_VALUES=Y

CRM_AGENTS_URL=
CRM_AGENTS_TIMEOUT=10
CRM_AGENTS_ALLOWED_DEPARTMENTS=52,24,43,51,38
